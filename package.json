{
  "name": "@dadi/api",
  "version": "1.16.2",
  "main": "main.js",
  "scripts": {
    "create-client": "cd ../../.. && node ./node_modules/@dadi/api/utils/create-client.js",
    "docs": "jsdoc -c ./docs/conf.json -R README.md -r dadi/lib -d docs",
    "commitmsg": "validate-commit-msg",
